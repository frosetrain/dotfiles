# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +5000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -5000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Screenshot
"sh ~/.config/rofi/applets/bin/screenshot.sh"
   Print

